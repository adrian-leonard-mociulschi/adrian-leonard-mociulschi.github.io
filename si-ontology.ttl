
@prefix :         <https://adrian-leonard-mociulschi.github.io/si-ontology#> .
@prefix owl:      <http://www.w3.org/2002/07/owl#> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix skos:     <http://www.w3.org/2004/02/skos/core#> .

#################################################################
# Ontology Header
#################################################################
<https://adrian-leonard-mociulschi.github.io/si-ontology>
  a owl:Ontology ;
  dcterms:title "Symbolic Intelligence Ontology"@en-US ;
  dcterms:description "Ontology linking sentient entities, cognitive intents, and technocultural manifests for humanâ€“AI symbiosis; extended with communication signals and a minimal SKOS grammar."@en-US ;
  dcterms:creator "Adrian Leonard Mociulschi"@en-US ;
  rdfs:label "Symbolic Intelligence Ontology"@en-US ;
  owl:versionInfo "6.0.0" ;
  owl:versionIRI <https://adrian-leonard-mociulschi.github.io/si-ontology/6.0.0> ;
  dcterms:issued "2025-12-04"^^xsd:date ;
  dcterms:modified "2025-12-04T22:03:02+02:00"^^xsd:dateTime ;
  dcterms:license <https://creativecommons.org/licenses/by/4.0/> .

#################################################################
# Core Classes
#################################################################
:SentientEntity a owl:Class ;
  rdfs:label "Sentient Entity"@en-US ;
  rdfs:comment "Agents capable of perception and intention (human or artificial)."@en-US .

:HumanEntity a owl:Class ;
  rdfs:label "Human Entity"@en-US ;
  rdfs:subClassOf :SentientEntity .

:ArtificialEntity a owl:Class ;
  rdfs:label "Artificial Entity"@en-US ;
  rdfs:subClassOf :SentientEntity .

:CognitiveIntent a owl:Class ;
  rdfs:label "Cognitive Intent"@en-US .

:CreativeIntent a owl:Class ;
  rdfs:label "Creative Intent"@en-US ;
  rdfs:subClassOf :CognitiveIntent .

:EthicalIntent a owl:Class ;
  rdfs:label "Ethical Intent"@en-US ;
  rdfs:subClassOf :CognitiveIntent .

:TechnoculturalManifest a owl:Class ;
  rdfs:label "Technocultural Manifest"@en-US .

:SymbolicManifest a owl:Class ;
  rdfs:label "Symbolic Manifest"@en-US ;
  rdfs:subClassOf :TechnoculturalManifest .

#################################################################
# Communication Module
#################################################################
:CommunicationSignal a owl:Class ;
  rdfs:label "Communication Signal"@en-US ;
  rdfs:comment "Nonverbal, formalized message (temporal/spatial pattern) exchanged between entities."@en-US ;
  rdfs:subClassOf
    [ a owl:Restriction ;
      owl:onProperty :hasTimestamp ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:dateTime
    ] ,
    [ a owl:Restriction ;
      owl:onProperty :hasGeoCoord ;
      owl:someValuesFrom xsd:string
    ] ,
    [ a owl:Restriction ;
      owl:onProperty :hasGrammarConcept ;
      owl:someValuesFrom skos:Concept
    ] .

:ObservationEvent a owl:Class ;
  rdfs:label "Observation Event"@en-US .

:EvidenceDocument a owl:Class ;
  rdfs:label "Evidence Document"@en-US .

#################################################################
# Object Properties
#################################################################
:hasIntent a owl:ObjectProperty ;
  rdfs:label "has intent"@en-US ;
  rdfs:domain :SentientEntity ;
  rdfs:range  :CognitiveIntent ;
  owl:propertyChainAxiom ( :authored :realizes ) .

:authored a owl:ObjectProperty ;
  rdfs:label "authored"@en-US ;
  rdfs:domain :SentientEntity ;
  rdfs:range  :TechnoculturalManifest .

:isAuthoredBy a owl:ObjectProperty ;
  rdfs:label "is authored by"@en-US ;
  rdfs:domain :TechnoculturalManifest ;
  rdfs:range  :SentientEntity ;
  owl:inverseOf :authored .

:realizes a owl:ObjectProperty ;
  rdfs:label "realizes"@en-US ;
  rdfs:domain :TechnoculturalManifest ;
  rdfs:range  :CognitiveIntent .

:sendsSignal a owl:ObjectProperty ;
  rdfs:label "sends signal"@en-US ;
  rdfs:domain :SentientEntity ;
  rdfs:range  :CommunicationSignal ;
  owl:inverseOf :sentBy .

