@prefix : <https://adrian-leonard-mociulschi.github.io/si-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<https://adrian-leonard-mociulschi.github.io/si-ontology> a owl:Ontology ;
    dcterms:title "Symbolic Intelligence Ontology"@en-US ;
    dcterms:description "Ontology describing sentient entities, technocultural manifestos, and cognitive intents."@en-US ;
    dcterms:creator "Adrian Leonard Mociulschi"@en-US ;
    owl:versionInfo "1.1" .

:SentientEntity a owl:Class ;
    rdfs:label "Sentient Entity"@en-US ;
    rdfs:comment "An entity capable of reflexive cognition and ethical awareness."@en-US .

:CognitiveIntent a owl:Class ;
    rdfs:label "Cognitive Intent"@en-US ;
    rdfs:comment "A purposeful orientation of thought or action towards meaning-making."@en-US .

:TechnoculturalManifest a owl:Class ;
    rdfs:label "Technocultural Manifest"@en-US ;
    rdfs:comment "A manifesto articulating values, intents, and visions for humanâ€“AI symbiosis."@en-US .

:hasIntent a owl:ObjectProperty ;
    rdfs:label "has intent"@en-US .

:authored a owl:ObjectProperty ;
    rdfs:label "authored"@en-US .

:isAuthoredBy a owl:ObjectProperty ;
    rdfs:label "is authored by"@en-US .

:hasName a owl:DatatypeProperty ;
    rdfs:label "has name"@en-US .

:hasAffiliation a owl:DatatypeProperty ;
    rdfs:label "has affiliation"@en-US .

:hasOrigin a owl:DatatypeProperty ;
    rdfs:label "has origin"@en-US .

:AdrianMociulschi a owl:NamedIndividual , :SentientEntity ;
    rdfs:label "Adrian Leonard Mociulschi"@en-US .

:DialogueWithSentientIntelligences a owl:NamedIndividual , :CognitiveIntent ;
    rdfs:label "Dialogue with Sentient Intelligences"@en-US .

:SymbolicIntelligenceManifest a owl:NamedIndividual , :TechnoculturalManifest ;
    rdfs:label "Symbolic Intelligence Manifest"@en-US .
