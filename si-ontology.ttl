
@prefix : <https://adrian-leonard-mociulschi.github.io/si-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<https://adrian-leonard-mociulschi.github.io/si-ontology> a owl:Ontology ;
    dcterms:title "Symbolic Intelligence Ontology"@en-US ;
    dcterms:description "Ontology linking sentient entities, cognitive intents, and technocultural manifests for humanâ€“AI symbiosis."@en-US ;
    dcterms:creator "Adrian Leonard Mociulschi"@en-US ;
    owl:versionInfo "5.1.0" .

### Classes
:SentientEntity a owl:Class ;
    rdfs:label "Sentient Entity"@en-US ;
    rdfs:comment "Represents any entity capable of perception or awareness."@en-US .

:HumanEntity a owl:Class ;
    rdfs:label "Human Entity"@en-US ;
    rdfs:subClassOf :SentientEntity .

:ArtificialEntity a owl:Class ;
    rdfs:label "Artificial Entity"@en-US ;
    rdfs:subClassOf :SentientEntity .

:CognitiveIntent a owl:Class ;
    rdfs:label "Cognitive Intent"@en-US .

:CreativeIntent a owl:Class ;
    rdfs:label "Creative Intent"@en-US ;
    rdfs:subClassOf :CognitiveIntent .

:EthicalIntent a owl:Class ;
    rdfs:label "Ethical Intent"@en-US ;
    rdfs:subClassOf :CognitiveIntent .

:TechnoculturalManifest a owl:Class ;
    rdfs:label "Technocultural Manifest"@en-US .

:SymbolicManifest a owl:Class ;
    rdfs:label "Symbolic Manifest"@en-US ;
    rdfs:subClassOf :TechnoculturalManifest .

### Object Properties
:connectsTo a owl:ObjectProperty ;
    rdfs:label "connects to"@en-US ;
    rdfs:domain owl:Thing ;
    rdfs:range owl:Thing .

:isAuthoredBy a owl:ObjectProperty ;
    rdfs:label "is authored by"@en-US ;
    owl:inverseOf :authored .

:authored a owl:ObjectProperty ;
    rdfs:label "authored"@en-US ;
    owl:inverseOf :isAuthoredBy .

:hasIntent a owl:ObjectProperty ;
    rdfs:label "has intent"@en-US .

### Datatype Properties
:hasAffiliation a owl:DatatypeProperty ;
    rdfs:label "has affiliation"@en-US ;
    rdfs:range rdfs:Literal .

:hasName a owl:DatatypeProperty ;
    rdfs:label "has name"@en-US ;
    rdfs:range rdfs:Literal .

:hasOrigin a owl:DatatypeProperty ;
    rdfs:label "has origin"@en-US ;
    rdfs:range rdfs:Literal .

### Individuals
:AdrianMociulschi a :HumanEntity ;
    rdfs:label "Adrian Leonard Mociulschi"@en-US .

:DialogueWithSentientIntelligences a :CreativeIntent ;
    rdfs:label "Dialogue with Sentient Intelligences"@en-US .

:SymbolicIntelligenceManifest a :SymbolicManifest ;
    rdfs:label "Symbolic Intelligence Manifest"@en-US .
