@prefix : <https://adrian-leonard-mociulschi.github.io/si-ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

#################################################################
# Ontology Metadata
#################################################################
: a owl:Ontology ;
    dcterms:title "Symbolic Intelligence Ontology"@en ;
    dcterms:description "Ontology linking sentient entities, cognitive intents, and technocultural manifests for human–AI symbiosis, with SKOS vocabulary."@en ;
    owl:versionInfo "2.1" .

#################################################################
# Classes
#################################################################
:SentientEntity a owl:Class ;
    rdfs:label "Sentient Entity"@en , "Entitate senzitivă"@ro , "Entité sentiente"@fr ;
    rdfs:comment "Entity capable of reflexive cognition and ethical awareness."@en .

:HumanEntity a owl:Class ;
    rdfs:subClassOf :SentientEntity ;
    rdfs:label "Human Entity"@en , "Entitate umană"@ro , "Entité humaine"@fr .

:ArtificialEntity a owl:Class ;
    rdfs:subClassOf :SentientEntity ;
    rdfs:label "Artificial Entity"@en , "Entitate artificială"@ro , "Entité artificielle"@fr ;
    rdfs:comment "AI/robotic agents participating in symbolic dialogue."@en .

:CognitiveIntent a owl:Class ;
    rdfs:label "Cognitive Intent"@en , "Intenție cognitivă"@ro , "Intention cognitive"@fr ;
    rdfs:comment "Purposeful orientation of thought or action towards meaning-making."@en .

:EthicalIntent a owl:Class ;
    rdfs:subClassOf :CognitiveIntent ;
    rdfs:label "Ethical Intent"@en , "Intenție etică"@ro , "Intention éthique"@fr .

:CreativeIntent a owl:Class ;
    rdfs:subClassOf :CognitiveIntent ;
    rdfs:label "Creative Intent"@en , "Intenție creativă"@ro , "Intention créative"@fr .

:TechnoculturalManifest a owl:Class ;
    rdfs:label "Technocultural Manifest"@en , "Manifest tehnocultural"@ro , "Manifeste technoculturel"@fr ;
    rdfs:comment "Manifest articulating values, intents, and visions for human–AI symbiosis."@en .

:SymbolicManifest a owl:Class ;
    rdfs:subClassOf :TechnoculturalManifest ;
    rdfs:label "Symbolic Manifest"@en , "Manifest simbolic"@ro , "Manifeste symbolique"@fr .

#################################################################
# Object Properties
#################################################################
:hasIntent a owl:ObjectProperty ;
    rdfs:domain :SentientEntity ;
    rdfs:range :CognitiveIntent ;
    rdfs:label "has intent"@en .

:authored a owl:ObjectProperty ;
    rdfs:domain :SentientEntity ;
    rdfs:range :TechnoculturalManifest ;
    owl:inverseOf :isAuthoredBy ;
    rdfs:label "authored"@en .

:isAuthoredBy a owl:ObjectProperty ;
    rdfs:domain :TechnoculturalManifest ;
    rdfs:range :SentientEntity ;
    rdfs:label "is authored by"@en .

#################################################################
# Datatype Properties
#################################################################
:hasName a owl:DatatypeProperty ;
    rdfs:domain :SentientEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has name"@en .

:hasAffiliation a owl:DatatypeProperty ;
    rdfs:domain :SentientEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has affiliation"@en .

:hasOrigin a owl:DatatypeProperty ;
    rdfs:domain :SentientEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has origin"@en .

#################################################################
# Individuals
#################################################################
:AdrianMociulschi a :HumanEntity ;
    rdfs:label "Adrian Leonard Mociulschi"@en ;
    :hasName "Adrian Leonard Mociulschi" ;
    :hasAffiliation "National University of Music Bucharest" ;
    :hasIntent :DialogueWithSentientIntelligences ;
    :authored :SymbolicIntelligenceManifest .

:DialogueWithSentientIntelligences a :EthicalIntent, :CreativeIntent ;
    rdfs:label "Dialogue with Sentient Intelligences"@en , "Dialog cu inteligențe senzitive"@ro , "Dialogue avec des intelligences sentientes"@fr .

:SymbolicIntelligenceManifest a :SymbolicManifest ;
    rdfs:label "Symbolic Intelligence Manifest"@en , "Manifestul Inteligenței Simbolice"@ro , "Manifeste de l’Intelligence Symbolique"@fr ;
    :isAuthoredBy :AdrianMociulschi .

#################################################################
# SKOS ConceptScheme
#################################################################
:SymbolicVocabulary a skos:ConceptScheme ;
    dcterms:title "Symbolic Vocabulary"@en , "Vocabular simbolic"@ro , "Vocabulaire symbolique"@fr ;
    dcterms:description "Controlled vocabulary for key notions in the ontology: entities, intents, manifests."@en .

#################################################################
# SKOS Concepts
#################################################################
:c_SentientEntity a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Sentient Entity"@en , "Entitate senzitivă"@ro , "Entité sentiente"@fr ;
    skos:altLabel "Agent of ethical awareness"@en ;
    skos:exactMatch :SentientEntity .

:c_HumanEntity a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Human Entity"@en , "Entitate umană"@ro , "Entité humaine"@fr ;
    skos:broader :c_SentientEntity ;
    skos:exactMatch :HumanEntity .

:c_ArtificialEntity a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Artificial Entity"@en , "Entitate artificială"@ro , "Entité artificielle"@fr ;
    skos:broader :c_SentientEntity ;
    skos:exactMatch :ArtificialEntity .

:c_CognitiveIntent a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Cognitive Intent"@en , "Intenție cognitivă"@ro , "Intention cognitive"@fr ;
    skos:altLabel "Purposeful orientation"@en ;
    skos:exactMatch :CognitiveIntent .

:c_EthicalIntent a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Ethical Intent"@en , "Intenție etică"@ro , "Intention éthique"@fr ;
    skos:broader :c_CognitiveIntent ;
    skos:narrower :c_CreativeIntent ;
    skos:exactMatch :EthicalIntent .

:c_CreativeIntent a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Creative Intent"@en , "Intenție creativă"@ro , "Intention créative"@fr ;
    skos:broader :c_CognitiveIntent ;
    skos:exactMatch :CreativeIntent .

:c_TechnoculturalManifest a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Technocultural Manifest"@en , "Manifest tehnocultural"@ro , "Manifeste technoculturel"@fr ;
    skos:exactMatch :TechnoculturalManifest .

:c_SymbolicManifest a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Symbolic Manifest"@en , "Manifest simbolic"@ro , "Manifeste symbolique"@fr ;
    skos:broader :c_TechnoculturalManifest ;
    skos:exactMatch :SymbolicManifest .
