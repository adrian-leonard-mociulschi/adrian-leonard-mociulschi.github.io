@prefix : <https://adrian-leonard-mociulschi.github.io/si-ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

#################################################################
# Ontology Metadata
#################################################################
: a owl:Ontology ;
    dcterms:title "Symbolic Intelligence Ontology"@en-US , "Ontologia Inteligenței Simbolice"@ro-RO , "Ontologie de l’Intelligence Symbolique"@fr-FR ;
    dcterms:description "Ontology linking sentient entities, cognitive intents, and technocultural manifests for human–AI symbiosis, with SKOS vocabulary."@en-US ;
    owl:versionInfo "3.0" .

#################################################################
# Classes
#################################################################
:SentientEntity a owl:Class ;
    rdfs:label "Sentient Entity"@en-US , "Entitate senzitivă"@ro-RO , "Entité sentiente"@fr-FR ;
    rdfs:comment "Entity capable of reflexive cognition and ethical awareness."@en-US .

:HumanEntity a owl:Class ;
    rdfs:subClassOf :SentientEntity ;
    rdfs:label "Human Entity"@en-US , "Entitate umană"@ro-RO , "Entité humaine"@fr-FR .

:ArtificialEntity a owl:Class ;
    rdfs:subClassOf :SentientEntity ;
    rdfs:label "Artificial Entity"@en-US , "Entitate artificială"@ro-RO , "Entité artificielle"@fr-FR ;
    rdfs:comment "AI/robotic agents participating in symbolic dialogue."@en-US .

:CognitiveIntent a owl:Class ;
    rdfs:label "Cognitive Intent"@en-US , "Intenție cognitivă"@ro-RO , "Intention cognitive"@fr-FR ;
    rdfs:comment "Purposeful orientation of thought or action towards meaning-making."@en-US .

:EthicalIntent a owl:Class ;
    rdfs:subClassOf :CognitiveIntent ;
    rdfs:label "Ethical Intent"@en-US , "Intenție etică"@ro-RO , "Intention éthique"@fr-FR .

:CreativeIntent a owl:Class ;
    rdfs:subClassOf :CognitiveIntent ;
    rdfs:label "Creative Intent"@en-US , "Intenție creativă"@ro-RO , "Intention créative"@fr-FR .

:TechnoculturalManifest a owl:Class ;
    rdfs:label "Technocultural Manifest"@en-US , "Manifest tehnocultural"@ro-RO , "Manifeste technoculturel"@fr-FR ;
    rdfs:comment "Manifest articulating values, intents, and visions for human–AI symbiosis."@en-US .

:SymbolicManifest a owl:Class ;
    rdfs:subClassOf :TechnoculturalManifest ;
    rdfs:label "Symbolic Manifest"@en-US , "Manifest simbolic"@ro-RO , "Manifeste symbolique"@fr-FR .

#################################################################
# Object Properties
#################################################################
:hasIntent a owl:ObjectProperty ;
    rdfs:domain :SentientEntity ;
    rdfs:range :CognitiveIntent ;
    rdfs:label "has intent"@en-US .

:authored a owl:ObjectProperty ;
    rdfs:domain :SentientEntity ;
    rdfs:range :TechnoculturalManifest ;
    owl:inverseOf :isAuthoredBy ;
    rdfs:label "authored"@en-US .

:isAuthoredBy a owl:ObjectProperty ;
    rdfs:domain :TechnoculturalManifest ;
    rdfs:range :SentientEntity ;
    rdfs:label "is authored by"@en-US .

#################################################################
# Datatype Properties
#################################################################
:hasName a owl:DatatypeProperty ;
    rdfs:domain :SentientEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has name"@en-US .

:hasAffiliation a owl:DatatypeProperty ;
    rdfs:domain :SentientEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has affiliation"@en-US .

:hasOrigin a owl:DatatypeProperty ;
    rdfs:domain :SentientEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has origin"@en-US .

#################################################################
# Individuals
#################################################################
:AdrianMociulschi a :HumanEntity ;
    rdfs:label "Adrian Leonard Mociulschi"@en-US , "Adrian Leonard Mociulschi"@ro-RO ;
    :hasName "Adrian Leonard Mociulschi"@en-US , "Adrian Leonard Mociulschi"@ro-RO ;
    :hasAffiliation "National University of Music Bucharest"@en-US , "Universitatea Națională de Muzică București"@ro-RO ;
    :hasIntent :DialogueWithSentientIntelligences ;
    :authored :SymbolicIntelligenceManifest .

:DialogueWithSentientIntelligences a :EthicalIntent, :CreativeIntent ;
    rdfs:label "Dialogue with Sentient Intelligences"@en-US , "Dialog cu inteligențe senzitive"@ro-RO , "Dialogue avec des intelligences sentientes"@fr-FR .

:SymbolicIntelligenceManifest a :SymbolicManifest ;
    rdfs:label "Symbolic Intelligence Manifest"@en-US , "Manifestul Inteligenței Simbolice"@ro-RO , "Manifeste de l’Intelligence Symbolique"@fr-FR ;
    :isAuthoredBy :AdrianMociulschi .

#################################################################
# SKOS ConceptScheme
#################################################################
:SymbolicVocabulary a skos:ConceptScheme ;
    dcterms:title "Symbolic Vocabulary"@en-US , "Vocabular simbolic"@ro-RO , "Vocabulaire symbolique"@fr-FR ;
    dcterms:description "Controlled vocabulary for key notions in the ontology: entities, intents, manifests."@en-US .

#################################################################
# SKOS Concepts
#################################################################
:c_SentientEntity a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Sentient Entity"@en-US , "Entitate senzitivă"@ro-RO , "Entité sentiente"@fr-FR ;
    skos:exactMatch :SentientEntity .

:c_HumanEntity a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Human Entity"@en-US , "Entitate umană"@ro-RO , "Entité humaine"@fr-FR ;
    skos:broader :c_SentientEntity ;
    skos:exactMatch :HumanEntity .

:c_ArtificialEntity a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Artificial Entity"@en-US , "Entitate artificială"@ro-RO , "Entité artificielle"@fr-FR ;
    skos:broader :c_SentientEntity ;
    skos:exactMatch :ArtificialEntity .

:c_CognitiveIntent a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Cognitive Intent"@en-US , "Intenție cognitivă"@ro-RO , "Intention cognitive"@fr-FR ;
    skos:exactMatch :CognitiveIntent .

:c_EthicalIntent a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Ethical Intent"@en-US , "Intenție etică"@ro-RO , "Intention éthique"@fr-FR ;
    skos:broader :c_CognitiveIntent ;
    skos:narrower :c_CreativeIntent ;
    skos:exactMatch :EthicalIntent .

:c_CreativeIntent a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Creative Intent"@en-US , "Intenție creativă"@ro-RO , "Intention créative"@fr-FR ;
    skos:broader :c_CognitiveIntent ;
    skos:exactMatch :CreativeIntent .

:c_TechnoculturalManifest a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Technocultural Manifest"@en-US , "Manifest tehnocultural"@ro-RO , "Manifeste technoculturel"@fr-FR ;
    skos:exactMatch :TechnoculturalManifest .

:c_SymbolicManifest a skos:Concept ;
    skos:inScheme :SymbolicVocabulary ;
    skos:prefLabel "Symbolic Manifest"@en-US , "Manifest simbolic"@ro-RO , "Manifeste symbolique"@fr-FR ;
    skos:broader :c_TechnoculturalManifest ;
    skos:exactMatch :SymbolicManifest .
