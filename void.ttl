@prefix void: <http://rdfs.org/ns/void#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <https://adrian-leonard-mociulschi.github.io/si-ontology#> .

#################################################################
# VoID Dataset Description
#################################################################
:Dataset a void:Dataset ;
    dc:title "Symbolic Intelligence Ontology"@en-US , "Ontologia Inteligenței Simbolice"@ro-RO , "Ontologie de l’Intelligence Symbolique"@fr-FR ;
    dc:description "An ontological manifesto inspired by Voyager probes, describing sentient entities, technocultural principles, and cognitive intents."@en-US ;
    dc:creator "Adrian Leonard Mociulschi"@en-US ;
    dc:publisher "GitHub Pages"@en-US ;
    dc:license <https://creativecommons.org/licenses/by/4.0/> ;
    dc:created "2025-11-11"^^xsd:date ;
    dc:subject "technoculture, symbolic intelligence, ontology, AI ethics"@en-US ;
    dc:language "en-US" , "ro-RO" , "fr-FR" ;

    #################################################################
    # Data Dumps
    #################################################################
    void:dataDump <https://adrian-leonard-mociulschi.github.io/si-ontology/si-ontology.owl> ;
    void:dataDump <https://adrian-leonard-mociulschi.github.io/si-ontology/si-ontology.ttl> ;

    #################################################################
    # Example Resources
    #################################################################
    void:exampleResource <https://adrian-leonard-mociulschi.github.io/si-ontology#AdrianMociulschi> ;
    void:exampleResource <https://adrian-leonard-mociulschi.github.io/manifest-si.xml> ;

    #################################################################
    # URI Space and Pattern
    #################################################################
    void:uriSpace "https://adrian-leonard-mociulschi.github.io/si-ontology#" ;
    void:uriRegexPattern "^https://adrian-leonard-mociulschi.github.io/si-ontology#.*" ;

    #################################################################
    # Vocabularies
    #################################################################
    void:vocabulary <http://www.w3.org/2002/07/owl#> ;
    void:vocabulary <http://www.w3.org/1999/02/22-rdf-syntax-ns#> ;
    void:vocabulary <http://www.w3.org/2000/01/rdf-schema#> ;
    void:vocabulary <http://www.w3.org/2004/02/skos/core#> ;

    #################################################################
    # Features
    #################################################################
    void:feature <http://www.w3.org/ns/sparql-service-description#SPARQLService> ;
    void:sparqlEndpoint <https://adrian-leonard-mociulschi.github.io/sparql> ;

    #################################################################
    # Triple Count
    #################################################################
    void:triples 150 .
