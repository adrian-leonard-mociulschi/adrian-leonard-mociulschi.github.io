<?xml version="1.0"?>
<rdf:RDF xmlns="https://adrian-leonard-mociulschi.github.io/si-ontology#"
         xml:base="https://adrian-leonard-mociulschi.github.io/si-ontology"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dcterms="http://purl.org/dc/terms/">

<owl:Ontology rdf:about="https://adrian-leonard-mociulschi.github.io/si-ontology">
    <dcterms:title xml:lang="en-US">Symbolic Intelligence Ontology</dcterms:title>
    <dcterms:description xml:lang="en-US">Ontology describing sentient entities, technocultural manifestos, and cognitive intents.</dcterms:description>
    <dcterms:creator xml:lang="en-US">Adrian Leonard Mociulschi</dcterms:creator>
    <owl:versionInfo>1.1</owl:versionInfo>
</owl:Ontology>

<owl:Class rdf:about="#SentientEntity">
    <rdfs:label xml:lang="en-US">Sentient Entity</rdfs:label>
    <rdfs:comment xml:lang="en-US">An entity capable of reflexive cognition and ethical awareness.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://adrian-leonard-mociulschi.github.io/si-ontology"/>
</owl:Class>

<owl:Class rdf:about="#CognitiveIntent">
    <rdfs:label xml:lang="en-US">Cognitive Intent</rdfs:label>
    <rdfs:comment xml:lang="en-US">A purposeful orientation of thought or action towards meaning-making.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://adrian-leonard-mociulschi.github.io/si-ontology"/>
</owl:Class>

<owl:Class rdf:about="#TechnoculturalManifest">
    <rdfs:label xml:lang="en-US">Technocultural Manifest</rdfs:label>
    <rdfs:comment xml:lang="en-US">A manifesto articulating values, intents, and visions for humanâ€“AI symbiosis.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://adrian-leonard-mociulschi.github.io/si-ontology"/>
</owl:Class>

<owl:ObjectProperty rdf:about="#hasIntent">
    <rdfs:label xml:lang="en-US">has intent</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#authored">
    <rdfs:label xml:lang="en-US">authored</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isAuthoredBy">
    <rdfs:label xml:lang="en-US">is authored by</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#connectsTo">
    <rdfs:label xml:lang="en-US">connects to</rdfs:label>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#hasName">
    <rdfs:label xml:lang="en-US">has name</rdfs:label>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasAffiliation">
    <rdfs:label xml:lang="en-US">has affiliation</rdfs:label>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasOrigin">
    <rdfs:label xml:lang="en-US">has origin</rdfs:label>
</owl:DatatypeProperty>

<owl:NamedIndividual rdf:about="#AdrianMociulschi">
    <rdf:type rdf:resource="#SentientEntity"/>
    <rdfs:label xml:lang="en-US">Adrian Leonard Mociulschi</rdfs:label>
    <connectsTo rdf:resource="#SentientEntity"/>
    <connectsTo rdf:resource="#CognitiveIntent"/>
    <connectsTo rdf:resource="#TechnoculturalManifest"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#DialogueWithSentientIntelligences">
    <rdf:type rdf:resource="#CognitiveIntent"/>
    <rdfs:label xml:lang="en-US">Dialogue with Sentient Intelligences</rdfs:label>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#SymbolicIntelligenceManifest">
    <rdf:type rdf:resource="#TechnoculturalManifest"/>
    <rdfs:label xml:lang="en-US">Symbolic Intelligence Manifest</rdfs:label>
    <rdfs:seeAlso rdf:resource="https://adrian-leonard-mociulschi.github.io/manifest-si.xml"/>
</owl:NamedIndividual>

</rdf:RDF>