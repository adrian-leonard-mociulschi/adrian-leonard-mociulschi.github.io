<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:void="http://rdfs.org/ns/void#"
    xmlns:dc="http://purl.org/dc/terms/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

  <void:Dataset rdf:about="https://adrian-leonard-mociulschi.github.io/si-ontology">
    <dc:title>Symbolic Intelligence Ontology</dc:title>
    <dc:description>An ontological manifesto inspired by Voyager probes, describing sentient entities, technocultural principles, and cognitive intents.</dc:description>
    <dc:creator>Adrian Leonard Mociulschi</dc:creator>
    <dc:publisher>GitHub Pages</dc:publisher>
    <dc:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
    <dc:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-11-11</dc:created>

    <!-- Data dumps -->
    <void:dataDump rdf:resource="https://adrian-leonard-mociulschi.github.io/si-ontology/si-ontology.owl"/>
    <void:dataDump rdf:resource="https://adrian-leonard-mociulschi.github.io/si-ontology/si-ontology.ttl"/>

    <!-- Example resource -->
    <void:exampleResource rdf:resource="https://adrian-leonard-mociulschi.github.io/si-ontology#AdrianMociulschi"/>

    <!-- URI space -->
    <void:uriSpace>https://adrian-leonard-mociulschi.github.io/si-ontology#</void:uriSpace>

    <!-- Vocabularies -->
    <void:vocabulary rdf:resource="http://www.w3.org/2002/07/owl#"/>
    <void:vocabulary rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
    <void:vocabulary rdf:resource="http://www.w3.org/2000/01/rdf-schema#"/>
    <void:vocabulary rdf:resource="http://www.w3.org/2004/02/skos/core#"/>

    <!-- Optional: number of triples -->
    <void:triples rdf:datatype="xsd:integer">150</void:triples>
  </void:Dataset>

</rdf:RDF>
