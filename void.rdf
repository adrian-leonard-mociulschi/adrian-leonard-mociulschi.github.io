<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:void="http://rdfs.org/ns/void#"
    xmlns:dc="http://purl.org/dc/terms/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

  <void:Dataset rdf:about="https://adrian-leonard-mociulschi.github.io/si-ontology">
    <dc:title xml:lang="en-US">Symbolic Intelligence Ontology</dc:title>
    <dc:title xml:lang="ro-RO">Ontologia Inteligenței Simbolice</dc:title>
    <dc:title xml:lang="fr-FR">Ontologie de l’Intelligence Symbolique</dc:title>

    <dc:description xml:lang="en-US">An ontological manifesto inspired by Voyager probes, describing sentient entities, technocultural principles, and cognitive intents.</dc:description>
    <dc:creator xml:lang="en-US">Adrian Leonard Mociulschi</dc:creator>
    <dc:publisher xml:lang="en-US">GitHub Pages</dc:publisher>
    <dc:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
    <dc:created rdf:datatype="xsd:date">2025-11-11</dc:created>
    <dc:subject xml:lang="en-US">technoculture, symbolic intelligence, ontology, AI ethics</dc:subject>

    <!-- Languages -->
    <dc:language>en-US</dc:language>
    <dc:language>ro-RO</dc:language>
    <dc:language>fr-FR</dc:language>

    <!-- Data dumps -->
    <void:dataDump rdf:resource="https://adrian-leonard-mociulschi.github.io/si-ontology/si-ontology.owl"/>
    <void:dataDump rdf:resource="https://adrian-leonard-mociulschi.github.io/si-ontology/si-ontology.ttl"/>

    <!-- Example resources -->
    <void:exampleResource rdf:resource="https://adrian-leonard-mociulschi.github.io/si-ontology#AdrianMociulschi"/>
    <void:exampleResource rdf:resource="https://adrian-leonard-mociulschi.github.io/manifest-si.xml"/>

    <!-- URI space and pattern -->
    <void:uriSpace>https://adrian-leonard-mociulschi.github.io/si-ontology#</void:uriSpace>
    <void:uriRegexPattern>^https://adrian-leonard-mociulschi.github.io/si-ontology#.*</void:uriRegexPattern>

    <!-- Vocabularies -->
    <void:vocabulary rdf:resource="http://www.w3.org/2002/07/owl#"/>
    <void:vocabulary rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
    <void:vocabulary rdf:resource="http://www.w3.org/2000/01/rdf-schema#"/>
    <void:vocabulary rdf:resource="http://www.w3.org/2004/02/skos/core#"/>

    <!-- Features -->
    <void:feature rdf:resource="http://www.w3.org/ns/sparql-service-description#SPARQLService"/>
    <void:sparqlEndpoint rdf:resource="https://adrian-leonard-mociulschi.github.io/sparql"/>

    <!-- Optional: number of triples -->
    <void:triples rdf:datatype="xsd:integer">150</void:triples>
  </void:Dataset>

</rdf:RDF>
