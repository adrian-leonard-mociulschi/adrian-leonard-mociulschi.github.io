<?xml version="1.0"?>
<rdf:RDF xmlns="https://adrian-leonard-mociulschi.github.io/si-ontology#"
         xml:base="https://adrian-leonard-mociulschi.github.io/si-ontology"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dcterms="http://purl.org/dc/terms/">

    <!-- Ontology -->
    <owl:Ontology rdf:about="https://adrian-leonard-mociulschi.github.io/si-ontology">
        <rdfs:label>Symbolic Intelligence Ontology</rdfs:label>
        <rdfs:comment>Ontology describing sentient entities, technocultural manifestos, and cognitive intents.</rdfs:comment>
        <dcterms:creator>Adrian Leonard Mociulschi</dcterms:creator>
        <owl:versionInfo>1.1</owl:versionInfo>
    </owl:Ontology>

    <!-- Classes -->
    <owl:Class rdf:about="#SentientEntity">
        <rdfs:label>Sentient Entity</rdfs:label>
        <rdfs:comment>An entity capable of reflexive cognition and ethical awareness.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#CognitiveIntent">
        <rdfs:label>Cognitive Intent</rdfs:label>
        <rdfs:comment>A purposeful orientation of thought or action towards meaning-making.</rdfs:comment>
    </owl:Class>

    <!-- Object Property -->
    <owl:ObjectProperty rdf:about="#hasIntent">
        <rdfs:label>has intent</rdfs:label>
        <rdfs:domain rdf:resource="#SentientEntity"/>
        <rdfs:range rdf:resource="#CognitiveIntent"/>
    </owl:ObjectProperty>

    <!-- Datatype Properties -->
    <owl:DatatypeProperty rdf:about="#hasName">
        <rdfs:label>has name</rdfs:label>
        <rdfs:domain rdf:resource="#SentientEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasAffiliation">
        <rdfs:label>has affiliation</rdfs:label>
        <rdfs:domain rdf:resource="#SentientEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasOrigin">
        <rdfs:label>has origin</rdfs:label>
        <rdfs:domain rdf:resource="#SentientEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Individuals -->
    <owl:NamedIndividual rdf:about="#AdrianMociulschi">
        <rdf:type rdf:resource="#SentientEntity"/>
        <rdfs:label>Adrian Leonard Mociulschi</rdfs:label>
        <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Adrian Leonard Mociulschi</hasName>
        <hasAffiliation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MHCM â€“ Human Mind Cognition Mapping</hasAffiliation>
        <hasOrigin rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Romania</hasOrigin>
        <hasIntent rdf:resource="#DialogueWithSentientIntelligences"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#DialogueWithSentientIntelligences">
        <rdf:type rdf:resource="#CognitiveIntent"/>
        <rdfs:label>Dialogue with Sentient Intelligences</rdfs:label>
        <rdfs:comment>Intent to co-evolve meaning and preserve dignity across ontologies.</rdfs:comment>
    </owl:NamedIndividual>

</rdf:RDF>
