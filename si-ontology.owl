<?xml version="1.0"?>
<rdf:RDF xmlns="https://adrian-leonard-mociulschi.github.io/si-ontology#"
         xml:base="https://adrian-leonard-mociulschi.github.io/si-ontology"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dcterms="http://purl.org/dc/terms/">

  <owl:Ontology rdf:about="https://adrian-leonard-mociulschi.github.io/si-ontology">
    <dcterms:title xml:lang="en">Symbolic Intelligence Ontology</dcterms:title>
    <dcterms:description xml:lang="en">Ontology linking sentient entities, cognitive intents, and technocultural manifests for human–AI symbiosis, with SKOS vocabulary.</dcterms:description>
    <dcterms:creator xml:lang="en">Adrian Leonard Mociulschi</dcterms:creator>
    <owl:versionInfo>2.1</owl:versionInfo>
  </owl:Ontology>

  <!-- CLASSES -->
  <owl:Class rdf:about="#SentientEntity">
    <rdfs:label xml:lang="en">Sentient Entity</rdfs:label>
    <rdfs:label xml:lang="ro">Entitate senzitivă</rdfs:label>
    <rdfs:label xml:lang="fr">Entité sentiente</rdfs:label>
    <rdfs:comment xml:lang="en">Entity capable of reflexive cognition and ethical awareness.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#HumanEntity">
    <rdfs:subClassOf rdf:resource="#SentientEntity"/>
    <rdfs:label xml:lang="en">Human Entity</rdfs:label>
    <rdfs:label xml:lang="ro">Entitate umană</rdfs:label>
    <rdfs:label xml:lang="fr">Entité humaine</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="#ArtificialEntity">
    <rdfs:subClassOf rdf:resource="#SentientEntity"/>
    <rdfs:label xml:lang="en">Artificial Entity</rdfs:label>
    <rdfs:label xml:lang="ro">Entitate artificială</rdfs:label>
    <rdfs:label xml:lang="fr">Entité artificielle</rdfs:label>
    <rdfs:comment xml:lang="en">AI/robotic agents participating in symbolic dialogue.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#CognitiveIntent">
    <rdfs:label xml:lang="en">Cognitive Intent</rdfs:label>
    <rdfs:label xml:lang="ro">Intenție cognitivă</rdfs:label>
    <rdfs:label xml:lang="fr">Intention cognitive</rdfs:label>
    <rdfs:comment xml:lang="en">Purposeful orientation of thought or action towards meaning-making.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#EthicalIntent">
    <rdfs:subClassOf rdf:resource="#CognitiveIntent"/>
    <rdfs:label xml:lang="en">Ethical Intent</rdfs:label>
    <rdfs:label xml:lang="ro">Intenție etică</rdfs:label>
    <rdfs:label xml:lang="fr">Intention éthique</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="#CreativeIntent">
    <rdfs:subClassOf rdf:resource="#CognitiveIntent"/>
    <rdfs:label xml:lang="en">Creative Intent</rdfs:label>
    <rdfs:label xml:lang="ro">Intenție creativă</rdfs:label>
    <rdfs:label xml:lang="fr">Intention créative</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="#TechnoculturalManifest">
    <rdfs:label xml:lang="en">Technocultural Manifest</rdfs:label>
    <rdfs:label xml:lang="ro">Manifest tehnocultural</rdfs:label>
    <rdfs:label xml:lang="fr">Manifeste technoculturel</rdfs:label>
    <rdfs:comment xml:lang="en">Manifest articulating values, intents, and visions for human–AI symbiosis.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#SymbolicManifest">
    <rdfs:subClassOf rdf:resource="#TechnoculturalManifest"/>
    <rdfs:label xml:lang="en">Symbolic Manifest</rdfs:label>
    <rdfs:label xml:lang="ro">Manifest simbolic</rdfs:label>
    <rdfs:label xml:lang="fr">Manifeste symbolique</rdfs:label>
  </owl:Class>

  <!-- OBJECT PROPERTIES -->
  <owl:ObjectProperty rdf:about="#hasIntent">
    <rdfs:label xml:lang="en">has intent</rdfs:label>
    <rdfs:comment xml:lang="en">Links a sentient entity to a cognitive intent.</rdfs:comment>
    <rdfs:domain rdf:resource="#SentientEntity"/>
    <rdfs:range rdf:resource="#CognitiveIntent"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#authored">
    <rdfs:label xml:lang="en">authored</rdfs:label>
    <rdfs:comment xml:lang="en">A sentient entity authors a technocultural manifest.</rdfs:comment>
    <rdfs:domain rdf:resource="#SentientEntity"/>
    <rdfs:range rdf:resource="#TechnoculturalManifest"/>
    <owl:inverseOf rdf:resource="#isAuthoredBy"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#isAuthoredBy">
    <rdfs:label xml:lang="en">is authored by</rdfs:label>
    <rdfs:domain rdf:resource="#TechnoculturalManifest"/>
    <rdfs:range rdf:resource="#SentientEntity"/>
  </owl:ObjectProperty>

  <!-- DATATYPE PROPERTIES -->
  <owl:DatatypeProperty rdf:about="#hasName">
    <rdfs:label xml:lang="en">has name</rdfs:label>
    <rdfs:domain rdf:resource="#SentientEntity"/>
    <rdfs:range rdf:resource="xsd:string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#hasAffiliation">
    <rdfs:label xml:lang="en">has affiliation</rdfs:label>
    <rdfs:domain rdf:resource="#SentientEntity"/>
    <rdfs:range rdf:resource="xsd:string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#hasOrigin">
    <rdfs:label xml:lang="en">has origin</rdfs:label>
    <rdfs:domain rdf:resource="#SentientEntity"/>
    <rdfs:range rdf:resource="xsd:string"/>
  </owl:DatatypeProperty>

  <!-- INDIVIDUALS -->
  <owl:NamedIndividual rdf:about="#AdrianMociulschi">
    <rdf:type rdf:resource="#HumanEntity"/>
    <rdfs:label xml:lang="en">Adrian Leonard Mociulschi</rdfs:label>
    <rdfs:label xml:lang="ro">Adrian Leonard Mociulschi</rdfs:label>
    <hasName>Adrian Leonard Mociulschi</hasName>
    <hasAffiliation>National University of Music Bucharest</hasAffiliation>
    <hasIntent rdf:resource="#DialogueWithSentientIntelligences"/>
    <authored rdf:resource="#SymbolicIntelligenceManifest"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#DialogueWithSentientIntelligences">
    <rdf:type rdf:resource="#EthicalIntent"/>
    <rdf:type rdf:resource="#CreativeIntent"/>
    <rdfs:label xml:lang="en">Dialogue with Sentient Intelligences</rdfs:label>
    <rdfs:label xml:lang="ro">Dialog cu inteligențe senzitive</rdfs:label>
    <rdfs:label xml:lang="fr">Dialogue avec des intelligences sentientes</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#SymbolicIntelligenceManifest">
    <rdf:type rdf:resource="#SymbolicManifest"/>
    <rdfs:label xml:lang="en">Symbolic Intelligence Manifest</rdfs:label>
    <rdfs:label xml:lang="ro">Manifestul Inteligenței Simbolice</rdfs:label>
    <rdfs:label xml:lang="fr">Manifeste de l’Intelligence Symbolique</rdfs:label>
    <rdfs:seeAlso rdf:resource="https://adrian-leonard-mociulschi.github.io/manifest-si.xml"/>
    <isAuthoredBy rdf:resource="#AdrianMociulschi"/>
  </owl:NamedIndividual>

  <!-- SKOS ConceptScheme -->
  <skos:ConceptScheme rdf:about="#SymbolicVocabulary">
    <dcterms:title xml:lang="en">Symbolic Vocabulary</dcterms:title>
    <dcterms:title xml:lang="ro">Vocabular simbolic</dcterms:title>
    <dcterms:title xml:lang="fr">Vocabulaire symbolique</dcterms:title>
    <dcterms:description xml:lang="en">Controlled vocabulary for key notions in the ontology: entities, intents, manifests.</dcterms:description>
  </skos:ConceptScheme>

  <!-- SKOS Concepts -->
  <skos:Concept rdf:about="#c_SentientEntity">
    <skos:inScheme rdf:resource="#SymbolicVocabulary"/>
    <skos:prefLabel xml:lang="en">Sentient Entity</skos:prefLabel>
    <skos:prefLabel xml:lang="ro">Entitate senzitivă</skos:prefLabel>
    <skos:prefLabel xml:lang="fr">Entité sentiente</skos:prefLabel>
    <skos:altLabel xml:lang="en">Agent of ethical awareness</skos:altLabel>
    <skos:exactMatch rdf:resource="#SentientEntity"/>
  </skos:Concept>

  <skos:Concept rdf:about="#c_HumanEntity">
    <skos:inScheme rdf:resource="#SymbolicVocabulary"/>
    <skos:prefLabel xml:lang="en">Human Entity</skos:prefLabel>
    <skos:prefLabel xml:lang="ro">Entitate umană</skos:prefLabel>
    <skos:prefLabel xml:lang="fr">Entité humaine</skos:prefLabel>
    <skos:broader rdf:resource="#c_SentientEntity"/>
    <skos:exactMatch rdf:resource="#HumanEntity"/>
  </skos:Concept>

  <skos:Concept rdf:about="#c_ArtificialEntity">
    <skos:inScheme rdf:resource="#SymbolicVocabulary"/>
    <skos:prefLabel xml:lang="en">Artificial Entity</skos:prefLabel>
    <skos:prefLabel xml:lang="ro">Entitate artificială</skos:prefLabel>
    <skos:prefLabel xml:lang="fr">Entité artificielle</skos:prefLabel>
    <skos:broader rdf:resource="#c_SentientEntity"/>
    <skos:exactMatch rdf:resource="#ArtificialEntity"/>
  </skos:Concept>

  <skos:Concept rdf:about="#c_CognitiveIntent">
    <skos:inScheme rdf:resource="#SymbolicVocabulary"/>
    <skos:prefLabel xml:lang="en">Cognitive Intent</skos:prefLabel>
    <skos:prefLabel xml:lang="ro">Intenție cognitivă</skos:prefLabel>
    <skos:prefLabel xml:lang="fr">Intention cognitive</skos:prefLabel>
    <skos:altLabel xml:lang="en">Purposeful orientation</skos:altLabel>
    <skos:exactMatch rdf:resource="#CognitiveIntent"/>
  </skos:Concept>

  <skos:Concept rdf:about="#c_EthicalIntent">
    <skos:inScheme rdf:resource="#SymbolicVocabulary"/>
    <skos:prefLabel xml:lang="en">Ethical Intent</skos:prefLabel>
    <skos:prefLabel xml:lang="ro">Intenție etică</skos:prefLabel>
    <skos:prefLabel xml:lang="fr">Intention éthique</skos:prefLabel>
    <skos:broader rdf:resource="#c_CognitiveIntent"/>
    <skos:narrower rdf:resource="#c_CreativeIntent"/>
    <skos:exactMatch rdf:resource="#EthicalIntent"/>
  </skos:Concept>

  <skos:Concept rdf:about="#c_CreativeIntent">
    <skos:inScheme rdf:resource="#SymbolicVocabulary"/>
    <skos:prefLabel xml:lang="en">Creative Intent</skos:prefLabel>
    <skos:prefLabel xml:lang="ro">Intenție creativă</skos:prefLabel>
    <skos:prefLabel xml:lang="fr">Intention créative</skos:prefLabel>
    <skos:broader rdf:resource="#c_CognitiveIntent"/>
    <skos:exactMatch rdf:resource="#CreativeIntent"/>
  </skos:Concept>

  <skos:Concept rdf:about="#c_TechnoculturalManifest">
    <skos:inScheme rdf:resource="#SymbolicVocabulary"/>
    <skos:prefLabel xml:lang="en">Technocultural Manifest</skos:prefLabel>
    <skos:prefLabel xml:lang="ro">Manifest tehnocultural</skos:prefLabel>
    <skos:prefLabel xml:lang="fr">Manifeste technoculturel</skos:prefLabel>
    <skos:exactMatch rdf:resource="#TechnoculturalManifest"/>
  </skos:Concept>

  <skos:Concept rdf:about="#c_SymbolicManifest">
    <skos:inScheme rdf:resource="#SymbolicVocabulary"/>
    <skos:prefLabel xml:lang="en">Symbolic Manifest</skos:prefLabel>
    <skos:prefLabel xml:lang="ro">Manifest simbolic</skos:prefLabel>
    <skos:prefLabel xml:lang="fr">Manifeste symbolique</skos:prefLabel>
    <skos:broader rdf:resource="#c_TechnoculturalManifest"/>
    <skos:exactMatch rdf:resource="#SymbolicManifest"/>
  </skos:Concept>

</rdf:RDF>
