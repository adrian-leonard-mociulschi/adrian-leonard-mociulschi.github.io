<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
  xmlns="https://adrian-leonard-mociulschi.github.io/si-ontology#"
  xml:base="https://adrian-leonard-mociulschi.github.io/si-ontology"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:skos="http://www.w3.org/2004/02/skos/core#">

  <!-- Ontology Header -->
  <owl:Ontology rdf:about="https://adrian-leonard-mociulschi.github.io/si-ontology">
    <dcterms:title xml:lang="en-US">Symbolic Intelligence Ontology</dcterms:title>
    <dcterms:description xml:lang="en-US">
      Ontology linking sentient entities, cognitive intents, and technocultural manifests for humanâ€“AI symbiosis; extended with communication signals and a minimal SKOS grammar.
    </dcterms:description>
    <dcterms:creator xml:lang="en-US">Adrian Leonard Mociulschi</dcterms:creator>
    <rdfs:label xml:lang="en-US">Symbolic Intelligence Ontology</rdfs:label>
    <owl:versionInfo>6.0.0</owl:versionInfo>
    <owl:versionIRI rdf:resource="https://adrian-leonard-mociulschi.github.io/si-ontology/6.0.0"/>
    <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-12-04</dcterms:issued>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-12-04T22:03:02+02:00</dcterms:modified>
    <dcterms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <owl:imports rdf:resource="http://purl.org/dc/terms/"/>
  </owl:Ontology>

  <!-- Core Classes -->
  <owl:Class rdf:about="#SentientEntity">
    <rdfs:label xml:lang="en-US">Sentient Entity</rdfs:label>
    <rdfs:comment xml:lang="en-US">Agents capable of perception and intention (human or artificial).</rdfs:comment>
    <!-- Existential restrictions -->
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasIntent"/>
        <owl:someValuesFrom rdf:resource="#CognitiveIntent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#authored"/>
        <owl:someValuesFrom rdf:resource="#TechnoculturalManifest"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#HumanEntity">
    <rdfs:label xml:lang="en-US">Human Entity</rdfs:label>
    <rdfs:subClassOf rdf:resource="#SentientEntity"/>
  </owl:Class>

  <owl:Class rdf:about="#ArtificialEntity">
    <rdfs:label xml:lang="en-US">Artificial Entity</rdfs:label>
