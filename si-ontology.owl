<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns="https://adrian-leonard-mociulschi.github.io/si-ontology#"
         xml:base="https://adrian-leonard-mociulschi.github.io/si-ontology"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dcterms="http://purl.org/dc/terms/">

  <owl:Ontology rdf:about="https://adrian-leonard-mociulschi.github.io/si-ontology">
    <dcterms:title xml:lang="en-US">Symbolic Intelligence Ontology</dcterms:title>
    <dcterms:title xml:lang="ro-RO">Ontologia Inteligenței Simbolice</dcterms:title>
    <dcterms:title xml:lang="fr-FR">Ontologie de l’Intelligence Symbolique</dcterms:title>
    <dcterms:description xml:lang="en-US">Ontology linking sentient entities, cognitive intents, and technocultural manifests for human–AI symbiosis.</dcterms:description>
    <dcterms:creator>Adrian Leonard Mociulschi</dcterms:creator>
    <owl:versionInfo>5.0.0</owl:versionInfo>
  </owl:Ontology>

  <!-- CLASSES -->
  <owl:Class rdf:about="#SentientEntity"/>
  <owl:Class rdf:about="#HumanEntity"><rdfs:subClassOf rdf:resource="#SentientEntity"/></owl:Class>
  <owl:Class rdf:about="#ArtificialEntity"><rdfs:subClassOf rdf:resource="#SentientEntity"/></owl:Class>
  <owl:Class rdf:about="#CognitiveIntent"/>
  <owl:Class rdf:about="#EthicalIntent"><rdfs:subClassOf rdf:resource="#CognitiveIntent"/></owl:Class>
  <owl:Class rdf:about="#CreativeIntent"><rdfs:subClassOf rdf:resource="#CognitiveIntent"/></owl:Class>
  <owl:Class rdf:about="#TechnoculturalManifest"/>
  <owl:Class rdf:about="#SymbolicManifest"><rdfs:subClassOf rdf:resource="#TechnoculturalManifest"/></owl:Class>

  <!-- OBJECT PROPERTIES -->
  <owl:ObjectProperty rdf:about="#hasIntent"/>
  <owl:ObjectProperty rdf:about="#authored"/>
  <owl:ObjectProperty rdf:about="#isAuthoredBy"/>

  <!-- DATATYPE PROPERTIES -->
  <owl:DatatypeProperty rdf:about="#hasName"/>
  <owl:DatatypeProperty rdf:about="#hasAffiliation"/>
  <owl:DatatypeProperty rdf:about="#hasOrigin"/>

  <!-- INDIVIDUALS -->
  <owl:NamedIndividual rdf:about="#AdrianMociulschi">
    <rdf:type rdf:resource="#HumanEntity"/>
    <hasName>Adrian Leonard Mociulschi</hasName>
    <hasAffiliation>Universitatea Națională de Muzică București</hasAffiliation>
    <hasIntent rdf:resource="#DialogueWithSentientIntelligences"/>
    <authored rdf:resource="#SymbolicIntelligenceManifest"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#DialogueWithSentientIntelligences">
    <rdf:type rdf:resource="#EthicalIntent"/>
    <rdf:type rdf:resource="#CreativeIntent"/>
    <rdfs:label xml:lang="ro-RO">Dialog cu inteligențe senzitive</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#SymbolicIntelligenceManifest">
    <rdf:type rdf:resource="#SymbolicManifest"/>
    <rdfs:label xml:lang="ro-RO">Manifestul Inteligenței Simbolice</rdfs:label>
    <rdfs:seeAlso rdf:resource="https://adrian-leonard-mociulschi.github.io/manifestsi.xml"/>
    <isAuthoredBy rdf:resource="#AdrianMociulschi"/>
  </owl:NamedIndividual>

</rdf:RDF>
