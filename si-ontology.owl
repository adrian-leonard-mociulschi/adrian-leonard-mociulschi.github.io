<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sh="http://www.w3.org/ns/shacl#"
         xmlns:prov="http://www.w3.org/ns/prov#"
         xmlns:ex="https://adrian-leonard-mociulschi.github.io/si-ontology#">

  <!-- Ontology Header -->
  <owl:Ontology rdf:about="https://adrian-leonard-mociulschi.github.io/si-ontology">
    <rdfs:label xml:lang="en-US">Symbolic Intelligence Ontology</rdfs:label>
    <owl:versionInfo>3.0</owl:versionInfo>
  </owl:Ontology>

  <!-- Classes -->
  <owl:Class rdf:about="ex:SentientEntity"/>
  <owl:Class rdf:about="ex:HumanEntity"><rdfs:subClassOf rdf:resource="ex:SentientEntity"/></owl:Class>
  <owl:Class rdf:about="ex:ArtificialEntity"><rdfs:subClassOf rdf:resource="ex:SentientEntity"/></owl:Class>
  <owl:Class rdf:about="ex:CognitiveIntent"/>
  <owl:Class rdf:about="ex:EthicalIntent"><rdfs:subClassOf rdf:resource="ex:CognitiveIntent"/></owl:Class>
  <owl:Class rdf:about="ex:CreativeIntent"><rdfs:subClassOf rdf:resource="ex:CognitiveIntent"/></owl:Class>
  <owl:Class rdf:about="ex:TechnoculturalManifest"/>
  <owl:Class rdf:about="ex:SymbolicManifest"><rdfs:subClassOf rdf:resource="ex:TechnoculturalManifest"/></owl:Class>

  <!-- Object Properties -->
  <owl:ObjectProperty rdf:about="ex:hasIntent"/>
  <owl:ObjectProperty rdf:about="ex:authored"/>
  <owl:ObjectProperty rdf:about="ex:isAuthoredBy"/>

  <!-- Datatype Properties -->
  <owl:DatatypeProperty rdf:about="ex:hasName"/>
  <owl:DatatypeProperty rdf:about="ex:hasAffiliation"/>
  <owl:DatatypeProperty rdf:about="ex:hasOrigin"/>

  <!-- Individuals with xml:lang -->
  <owl:NamedIndividual rdf:about="ex:AdrianMociulschi">
    <rdf:type rdf:resource="ex:HumanEntity"/>
    <ex:hasName xml:lang="en-US">Adrian Leonard Mociulschi</ex:hasName>
    <ex:hasAffiliation xml:lang="en-US">National University of Music Bucharest</ex:hasAffiliation>
    <ex:hasIntent rdf:resource="ex:DialogueWithSentientIntelligences"/>
    <ex:authored rdf:resource="ex:SymbolicIntelligenceManifest"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="ex:DialogueWithSentientIntelligences">
    <rdf:type rdf:resource="ex:EthicalIntent"/>
    <rdf:type rdf:resource="ex:CreativeIntent"/>
    <rdfs:label xml:lang="en-US">Dialogue with Sentient Intelligences</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="ex:SymbolicIntelligenceManifest">
    <rdf:type rdf:resource="ex:SymbolicManifest"/>
    <rdfs:label xml:lang="en-US">Symbolic Intelligence Manifest</rdfs:label>
    <ex:isAuthoredBy rdf:resource="ex:AdrianMociulschi"/>
  </owl:NamedIndividual>

  <!-- SHACL Shapes -->
  <sh:NodeShape rdf:about="ex:SentientEntityShape">
    <sh:targetClass rdf:resource="ex:SentientEntity"/>
    <sh:property><sh:PropertyShape><sh:path rdf:resource="ex:hasIntent"/><sh:minCount rdf:datatype="xsd:integer">1</sh:minCount><sh:maxCount rdf:datatype="xsd:integer">3</sh:maxCount></sh:PropertyShape></sh:property>
    <sh:property><sh:PropertyShape><sh:path rdf:resource="ex:hasName"/><sh:datatype rdf:resource="xsd:string"/></sh:PropertyShape></sh:property>
    <sh:property><sh:PropertyShape><sh:path rdf:resource="ex:hasAffiliation"/><sh:datatype rdf:resource="xsd:string"/></sh:PropertyShape></sh:property>
    <sh:property><sh:PropertyShape><sh:path rdf:resource="ex:hasOrigin"/><sh:datatype rdf:resource="xsd:string"/></sh:PropertyShape></sh:property>
  </sh:NodeShape>

  <!-- PROV-O Integration -->
  <prov:Entity rdf:about="ex:SymbolicIntelligenceManifest"><prov:wasAttributedTo rdf:resource="ex:AdrianMociulschi"/></prov:Entity>
  <prov:Agent rdf:about="ex:AdrianMociulschi"/>
  <prov:Activity rdf:about="ex:authored"><prov:wasAssociatedWith rdf:resource="ex:AdrianMociulschi"/><prov:generated rdf:resource="ex:SymbolicIntelligenceManifest"/></prov:Activity>

</rdf:RDF>
